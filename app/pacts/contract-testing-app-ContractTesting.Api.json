      "providerStates": [
        {
          "name": "product with id 1 exists"
        }
      ],
      "request": {
        "body": {
          "description": "Updated product description",
          "name": "Updated Product Name",
          "price": 29.99
        },
        "headers": {
          "Accept": "application/json",
          "Content-Type": "application/json"
        },
        "method": "PUT",
        "path": "/api/products/1"
      },
      "response": {
        "body": {
          "description": "Updated product description",
          "id": 1,
          "name": "Updated Product Name",
          "price": 29.99
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$.description": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            },
            "$.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.price": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "decimal"
                }
              ]
            }
          },
          "header": {},
          "status": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "15.0.1"
    },
